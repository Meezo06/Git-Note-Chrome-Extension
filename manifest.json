{
    "manifest_version": 3,
    "name": "Git-Note",
    "version": "1.0",
    "description": "Make notes and highlight Github repositories",
    "permissions": ["tabs", "contextMenus", "scripting", "storage"],
    "background": {
        "service_worker": "serviceWorker.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": ["https://github.com/*"]
}